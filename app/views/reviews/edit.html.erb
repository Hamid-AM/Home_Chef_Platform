<h1>Edit Review</h1>
<%= simple_form_for([@booking, @review]) do |f| %>
  <div class="form-group">
    <%= f.input :rating, as: :select, collection: (1..5), label: 'Rating' %>
    <%= f.input :comment, as: :text, label: 'Comment' %>
  </div>
  <div class="form-group">
    <%= f.submit %>
  </div>
<% end %>
